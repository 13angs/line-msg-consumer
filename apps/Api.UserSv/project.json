{
    "name": "Api.UserSv",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "sourceRoot": "apps/Api.UserSv",
    "targets": {
        "build": {
            "executor": "@nx-dotnet/core:build",
            "outputs": [
                "{workspaceRoot}/dist/apps/Api.UserSv",
                "{workspaceRoot}/dist/intermediates/apps/Api.UserSv"
            ],
            "options": {
                "configuration": "Debug",
                "noDependencies": true
            },
            "configurations": {
                "production": {
                    "configuration": "Release"
                }
            }
        },
        "serve": {
            "executor": "@nx-dotnet/core:serve",
            "options": {
                "configuration": "Debug"
            },
            "configurations": {
                "production": {
                    "configuration": "Release"
                }
            }
        },
        "lint": {
            "executor": "@nx-dotnet/core:format"
        },
        "swagger": {
            "executor": "@nx-dotnet/core:update-swagger",
            "outputs": ["options.output"],
            "options": {
                "output": "libs/generated/api.user-sv-swagger/swagger.json"
            },
            "dependsOn": ["build"]
        }
    },
    "tags": []
}
