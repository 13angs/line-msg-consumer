deployment:
  name: lmc-api-platform-depl
  image:
    repo: 13angs/lmc.api.platform.sv
    tag: latest
  env:
    rabbitMQConfig:
      properties:
        prefetchCount: 100
    aspnetCoreEnv: 'Development'

service:
  name: lmc-api-platform-sv
  ports:
    default: 5000
    grpc: 7000
    nodePort: 30104
  type: NodePort